#!/bin/bash  # not for execution, but vim syntax hinting
#

ME=${BASH_SOURCE[1]} # This should be the script that called .bash_footer, BASH_SOURCE[0] /IS/ .bash_footer

printf "$(date): $(printf "%s[%d]" $(ps -o comm=,pid= $$)): Closing '$ME'\n" >> ~/.bash_log

# If we are the original source, clean up our tracking variables
if [ "$FIRST" == "$ME" ]
then
    unset FIRST SEEN
fi

unset ME
